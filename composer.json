{
    "name": "aplr/laravel-paypal",
    "description": "Paypal SDK Bridge for Laravel",
    "keywords": ["laravel", "paypal", "paypal-php-sdk"],
    "license": "MIT",
    "support": {
        "issues": "https://gitlab.aplr.me/aplr/laravel-paypal/issues",
        "source": "https://gitlab.aplr.me/aplr/laravel-paypal"
    },
    "authors": [
        {
            "name": "Andreas Pfurtscheller",
            "email": "hello@aplr.me"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "5.*",
        "paypal/rest-api-sdk-php": "dev-2.0-beta"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Aplr\\LaravelBraintree\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aplr\\LaravelBraintree\\ServiceProvider"
            ],
            "aliases": {
                "Braintree": "Aplr\\LaravelBraintree\\Facade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}